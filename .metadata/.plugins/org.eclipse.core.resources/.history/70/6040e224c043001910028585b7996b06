package 반복문;

public class 별찍기 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=0;i<5;i++) {
			for(int k=5;k<=k;k--) {
				System.out.print(" ");
			}
			for(int j=0;j<=i;j++) {
				System.out.print("*");
			}
		}
			for(int l=6;l<9;l++) {
				for(int m=9;m<=m;m--) {
					System.out.print(" ");
				}
				for(int n=0;n<=6;n++) {
					System.out.print("*");
		
				}
			System.out.println();
		}
	}
}
